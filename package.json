{
  "name": "ewiv",
  "displayName": "ewiv",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.48.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:ewiv.helloWorld"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ewiv.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "ewiv.refresh",
        "title": "refressh",
        "icon": "$(refresh)"
      },
      {
        "command": "ewiv.add_site",
        "title":"add site",
        "icon":"$(add)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "mwHistoryExplorer",
          "title": "history-explorer",
          "icon": "images/icon.svg"
        }
      ]
    },
    "views": {
      "mwHistoryExplorer": [
        {
          "id": "mwRecentChange",
          "name": "recent change",
          "contextualTitle": "Recent Change"
        },
        {
          "id": "mwPageRevisions",
          "name": "page revisions",
          "contextualTitle": "Page Revision"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "ewiv.refresh",
          "group": "navigation"
        },
        {
          "command": "ewiv.add_site",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.48.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.27",
    "eslint": "^7.6.0",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "glob": "^7.1.6",
    "mocha": "^8.0.1",
    "typescript": "^3.8.3",
    "vscode-test": "^1.4.0"
  }
}
